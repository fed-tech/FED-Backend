// This file contains the user schema



// Define your user model
model User {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  email         String   @unique
  name          String
  access        Int      @default(2)
  year          String
  password      String
  rollNumber    String
  school        String
  college       String
  contactNo     String 
  whatsappNo    String 
  member        Json?
  forms         Json?
  // Name of the collection -> userdb
  @@map("userdb")
}
