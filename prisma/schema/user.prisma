enum AccessTypes {
  ADMIN

  PRESIDENT
  VICEPRESIDENT

  USER

  DIRECTOR_CREATIVE
  DIRECTOR_TECHNICAL
  DIRECTOR_MARKETING
  DIRECTOR_OPERATIONS
  DIRECTOR_SPONSORSHIP

  CREATIVE
  TECHNICAL
  MARKETING
  OPERATIONS
  SPONSORSHIP
  ALUMNI
}

model user {
  id               String             @id @default(auto()) @map("_id") @db.ObjectId
  email            String             @unique
  name             String?
  access           AccessTypes        @default(USER)
  year             String?
  img              String?
  blurhash         String?
  password         String
  rollNumber       String?
  school           String?
  college          String?
  contactNo        String?
  whatsappNo       String?
  extra            Json?
  regForm          String[]           @default([])
  editProfileCount Int?
  userForms        formRegistration[]

  @@map("userdb")
}
